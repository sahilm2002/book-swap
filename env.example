# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Books API Configuration
NEXT_PUBLIC_GOOGLE_BOOKS_API_KEY=your_google_books_api_key_here
# Alternative: Server-side only (more secure)
GOOGLE_BOOKS_API_KEY=your_google_books_api_key_here

# Book Cover Fetching Configuration (optional)
BOOK_COVERS_CONCURRENCY=3
BOOK_COVERS_CACHE_TTL=3600000
BOOK_COVERS_MAX_RETRIES=3
BOOK_COVERS_RETRY_DELAY=1000

# Database Configuration
DATABASE_URL=your_database_url_here

# Authentication
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Environment
NODE_ENV=development
